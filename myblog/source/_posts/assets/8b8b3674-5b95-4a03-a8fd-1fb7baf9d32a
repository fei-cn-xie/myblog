   		<script>
	    	(function($) {
	    		$(document).ready(function() {
	    			$.each($("textarea"), function(i, n){
	    				$(n).css("height", n.scrollHeight + "px");
	    			});

    			    var height;
    			    var subHeight = $('#sub-banner').height();
    			    $(window).scroll(function(){
    			    	height = $(this).scrollTop();  
    			        
    			        if(parseFloat(height) > subHeight){
    			        	$("#inner-sidebar").addClass("fixed");
    			        }else{
    			        	$("#inner-sidebar").removeClass("fixed");
    			        }
    			    });
    			    
    			    mergeCells('zsml-table', 1, 0, 3, true); // 参数依次表示：表格ID，从第一行开始，到最后一行，第二列
    			    mergeCells('zsml-table', 1, 0, 2, true); // 参数依次表示：表格ID，从第一行开始，到最后一行，第二列
	    		});
	    	})(jQuery);
	    	
	    	 /**
	         * 合并单元格
	         * @param tableId   表格的ID
	         * @param startRow  起始行
	         * @param endRow    结束行:0代表合并所有行
	         * @param colIndex  合并的列号，对第几列进行合并(从0开始)。第一行从0开始
	         * @param mergeBlank是否合并空值单元格，默认不合并
	         */
			function mergeCells(tableId, startRow, endRow, colIndex, mergeBlank) {
	    		var tb = document.getElementById(tableId);
	            if(!tb || !tb.rows || tb.rows.length <= 0) {
	                return;
	            }
	            if(colIndex >= tb.rows[0].cells.length || (startRow >= endRow && endRow != 0)) {
	                return;
	            }
	            if(endRow == 0) {
	                endRow = tb.rows.length - 1;
	            }
	            for(var i = startRow; i < endRow; i++) {
	            	if((!mergeBlank && (!tb.rows[startRow].cells[colIndex] || tb.rows[startRow].cells[colIndex].innerHTML == '')) 
	            			|| (!tb.rows[startRow].cells[colIndex] || !tb.rows[i + 1].cells[colIndex])) {
	            		mergeCells(tableId, i + 1, endRow, colIndex, mergeBlank);
	                    break;
	            	} 
	                if(tb.rows[startRow].cells[colIndex].getAttribute('merge') && tb.rows[i + 1].cells[colIndex].getAttribute('merge') && tb.rows[startRow].cells[colIndex].innerHTML == tb.rows[i + 1].cells[colIndex].innerHTML) { //如果相等就合并单元格,合并之后跳过下一行
	                    tb.rows[i + 1].removeChild(tb.rows[i + 1].cells[colIndex]);
	                    tb.rows[startRow].cells[colIndex].rowSpan = (tb.rows[startRow].cells[colIndex].rowSpan) + 1;
	                } else {
	                    mergeCells(tableId, i + 1, endRow, colIndex, mergeBlank);
	                    break;
	                } 
	            }
	    	}

			
			$('.xmjjinfo').click(function() {
				var newWindow = window.open('/publish/infors/view/xmjj/'+$(this).data('val'));
				newWindow.opener = null;
			});
	    </script>
    </body>
